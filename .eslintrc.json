{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
        "react/prefer-stateless-function": "off",
        "react/prop-types": "off"
    },
    "settings": {
        "import/resolver": "webpack"
    },
    "env": {
        "browser": true
    }
}