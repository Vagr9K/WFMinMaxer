{
  "name": "wfminmaxer",
  "description": "A webapp for comparing stats of Warfame enemies.",
  "version": "0.1.0",
  "main": "index.js",
  "repository": "https://github.com/Vagr9K/WFMinMaxer",
  "author": "Ruben Harutyunyan <harutyunyan.ruben@gmail.com>",
  "license": "AGPL-3.0",
  "scripts": {
    "clean": "rm -rf dist.dev/* && rm -rf dist.prod/",
    "serve": "webpack-dev-server --content-base dist.dev/ --inline --hot --colors -d",
    "build": "NODE_ENV='production' webpack -p --progress --display-chunks --colors",
    "build-dev": "webpack --progress -d --display-chunks",
    "bundle-stats": "NODE_ENV='production' webpack -p --progress --display-chunks --colors --json > bundle-stats.json",
    "bundle-stats-analyze": "npm run bundle-stats && cat bundle-stats.json | webpack-bundle-size-analyzer",
    "gh-deploy": "gh-pages -d dist.prod"
  },
  "dependencies": {
    "chart.js": "^2.5.0",
    "html-webpack-plugin": "^2.28.0",
    "immutable": "^3.8.1",
    "normalize.css": "^5.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-md": "^1.0.15",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.22.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "gh-pages": "^1.0.0",
    "node-sass": "^4.5.3",
    "path": "^0.12.7",
    "postcss-loader": "^2.0.6",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.13.1",
    "stylefmt": "^6.0.0",
    "stylelint": "^7.11.0",
    "stylelint-config-standard": "^16.0.0",
    "webpack": "^2.6.1",
    "webpack-bundle-size-analyzer": "^2.7.0",
    "webpack-dev-server": "^2.4.5",
    "webpack-merge": "^4.1.0"
  }
}